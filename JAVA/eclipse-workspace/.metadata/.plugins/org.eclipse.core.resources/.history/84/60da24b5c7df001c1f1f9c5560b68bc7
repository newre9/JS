package projem;

public class Second {
	
		static int myMethod(int a, int b) {
		return a+b;
		}
		
	public static void main(String[] args) {
		First myobje=new First();//obje olu≈üturdum.
		System.out.println("objeden gelen deger:" + myobje.a);
		int x= myMethod(3,6);
		System.out.println(x);
		
	}
	

}
